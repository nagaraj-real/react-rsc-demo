{
  "name": "reactssr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/cli": "^7.0.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "babel:build": "babel src --out-dir lib --extensions .ts,.js,.tsx,.jsx,.cjs,.mjs --presets=@babel/preset-env,@babel/preset-react",
    "server:start": "node ./lib/server.js",
    "start": "npm run babel:build && npm run server:start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
